#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eduardo/windows-11-web/node_modules/.pnpm/@turbo+gen@1.10.16_@types+node@20.10.2_typescript@5.3.2/node_modules/@turbo/gen/dist/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/@turbo+gen@1.10.16_@types+node@20.10.2_typescript@5.3.2/node_modules/@turbo/gen/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/@turbo+gen@1.10.16_@types+node@20.10.2_typescript@5.3.2/node_modules/@turbo/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/@turbo+gen@1.10.16_@types+node@20.10.2_typescript@5.3.2/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eduardo/windows-11-web/node_modules/.pnpm/@turbo+gen@1.10.16_@types+node@20.10.2_typescript@5.3.2/node_modules/@turbo/gen/dist/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/@turbo+gen@1.10.16_@types+node@20.10.2_typescript@5.3.2/node_modules/@turbo/gen/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/@turbo+gen@1.10.16_@types+node@20.10.2_typescript@5.3.2/node_modules/@turbo/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/@turbo+gen@1.10.16_@types+node@20.10.2_typescript@5.3.2/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@turbo/gen/dist/cli.js" "$@"
else
  exec node  "$basedir/../@turbo/gen/dist/cli.js" "$@"
fi
