#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules/million/packages/cli/dist/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules/million/packages/cli/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules/million/packages/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules/million/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules/million/packages/cli/dist/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules/million/packages/cli/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules/million/packages/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules/million/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/million@2.6.4/node_modules:/home/eduardo/windows-11-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../million/packages/cli/dist/index.js" "$@"
else
  exec node  "$basedir/../million/packages/cli/dist/index.js" "$@"
fi
