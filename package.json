{
	"name": "windows-11-web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev-debug": "NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint --fix",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
		"type-check": "tsc --noEmit",
		"test": "jest --passWithNoTests",
		"cypress:open": "start-server-and-test start http://localhost:3000 'cypress open'",
		"cypress:headless": "start-server-and-test start http://localhost:3000 'cypress run --spec cypress/e2e/auth'",
		"test:e2e": "if [ -f .next/BUILD_ID ]; then yarn cypress:open; else yarn build && yarn cypress:open; fi",
		"test:e2e:headless": "if [ -f .next/BUILD_ID ]; then yarn cypress:headless; else yarn build &&  yarn cypress:headless; fi",
		"theme": "chakra-cli tokens src/theme/index.ts",
		"theme:watch": "chakra-cli tokens src/theme/index.ts --watch",
		"plop": "plop",
		"prepare": "husky install; yarn theme"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn test:e2e:headless; yarn type-check"
		}
	},
	"dependencies": {
		"@chakra-ui/next-js": "^2.1.5",
		"@chakra-ui/react": "^2.8.0",
		"@chakra-ui/theme-tools": "^2.1.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@types/node": "20.5.1",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"axios": "^1.4.0",
		"chakra-react-select": "^4.7.0",
		"eslint": "8.47.0",
		"eslint-config-next": "^13.4.19",
		"framer-motion": "^10.16.1",
		"lodash.memoize": "^4.1.2",
		"million": "^2.6.0-beta.4",
		"next": "13.4.20-canary.0",
		"next-auth": "4.23.1",
		"nextjs-toploader": "^1.4.2",
		"proxy-from-env": "^1.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.45.4",
		"react-icons": "^4.10.1",
		"react-toastify": "^9.1.3",
		"sharp": "^0.32.5",
		"swr": "^2.2.1",
		"typescript": "5.2.2"
	},
	"devDependencies": {
		"@chakra-ui/anatomy": "^2.2.0",
		"@chakra-ui/cli": "^2.4.1",
		"@chakra-ui/styled-system": "^2.9.1",
		"@next/eslint-plugin-next": "^13.4.19",
		"@testing-library/jest-dom": "^6.0.1",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/lodash.memoize": "^4.1.7",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^6.4.0",
		"cypress": "^12.17.4",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-cypress": "^2.14.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jest": "^27.2.3",
		"eslint-plugin-jest-dom": "^5.0.2",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-no-inline-styles": "^1.0.5",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-perf": "^3.3.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-testing-library": "^6.0.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"jest": "^29.6.3",
		"jest-environment-jsdom": "^29.6.3",
		"lint-staged": "^14.0.1",
		"plop": "^3.1.2",
		"prettier": "^3.0.2",
		"prettier-plugin-jsdoc": "^1.0.1",
		"start-server-and-test": "^2.0.0"
	},
	"overrides": {
		"@rushstack/eslint-patch": "^1.3.2"
	},
	"resolutions": {
		"@rushstack/eslint-patch": "^1.3.2"
	}
}
