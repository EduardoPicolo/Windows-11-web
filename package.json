{
	"name": "windows-11-web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "NODE_OPTIONS='--max-old-space-size=12288' next dev",
		"dev-debug": "NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint --fix",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
		"type-check": "tsc --noEmit",
		"test": "jest --passWithNoTests",
		"cypress:e2e:open": "start-server-and-test 'yarn start -p 3001' http://localhost:3001 'cypress open'",
		"cypress:e2e:headless": "start-server-and-test 'yarn start -p 3001' http://localhost:3001 'cypress run'",
		"cypress:component:open": "start-server-and-test 'yarn start -p 3001' http://localhost:3001 'cypress open --component'",
		"cypress:component:headless": "start-server-and-test 'yarn start -p 3001' http://localhost:3001 'cypress run --component'",
		"test:e2e": "if [ -f .next/BUILD_ID ]; then yarn cypress:e2e:open; else yarn build && yarn cypress:e2e:open; fi",
		"test:e2e:headless": "if [ -f .next/BUILD_ID ]; then yarn cypress:e2e:headless; else yarn build &&  yarn cypress:e2e:headless; fi",
		"test:component": "if [ -f .next/BUILD_ID ]; then yarn cypress:component:open; else yarn build && yarn cypress:component:open; fi",
		"test:component:headless": "if [ -f .next/BUILD_ID ]; then yarn cypress:component:headless; else yarn build &&  yarn cypress:component:headless; fi",
		"theme": "chakra-cli tokens src/theme/index.ts",
		"theme:watch": "chakra-cli tokens src/theme/index.ts --watch",
		"plop": "plop",
		"prepare": "husky install; yarn theme"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn test:component:headless; yarn type-check"
		}
	},
	"dependencies": {
		"@chakra-ui/next-js": "^2.1.5",
		"@chakra-ui/react": "^2.8.1",
		"@chakra-ui/react-use-size": "^2.1.0",
		"@chakra-ui/theme-tools": "^2.1.1",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@formkit/auto-animate": "^0.8.0",
		"@types/node": "20.6.3",
		"@types/react": "18.2.22",
		"@types/react-dom": "18.2.7",
		"@vercel/analytics": "^1.0.2",
		"axios": "^1.5.0",
		"chakra-react-select": "^4.7.2",
		"dragselect": "^2.7.4",
		"eslint": "8.50.0",
		"eslint-config-next": "^13.5.2",
		"framer-motion": "^10.16.4",
		"lodash.memoize": "^4.1.2",
		"million": "^2.6.0-beta.4",
		"next": "13.5.2",
		"next-auth": "4.23.1",
		"nextjs-toploader": "^1.4.2",
		"proxy-from-env": "^1.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.46.2",
		"react-icons": "^4.11.0",
		"react-moveable": "^0.54.2",
		"react-rnd": "^10.4.1",
		"react-selecto": "^1.26.0",
		"react-toastify": "^9.1.3",
		"sharp": "^0.32.6",
		"swr": "^2.2.2",
		"typescript": "5.2.2"
	},
	"devDependencies": {
		"@chakra-ui/anatomy": "^2.2.1",
		"@chakra-ui/cli": "^2.4.1",
		"@chakra-ui/styled-system": "^2.9.1",
		"@next/eslint-plugin-next": "^13.5.2",
		"@testing-library/jest-dom": "^6.1.3",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.5.1",
		"@types/lodash.memoize": "^4.1.7",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"cypress": "^13.2.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-cypress": "^2.15.1",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jest": "^27.4.0",
		"eslint-plugin-jest-dom": "^5.0.2",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-no-inline-styles": "^1.0.5",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-perf": "^3.3.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-testing-library": "^6.0.2",
		"eslint-plugin-unicorn": "^48.0.1",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"lint-staged": "^14.0.1",
		"plop": "^3.1.2",
		"prettier": "^3.0.3",
		"prettier-plugin-jsdoc": "^1.0.2",
		"start-server-and-test": "^2.0.1"
	},
	"overrides": {
		"@rushstack/eslint-patch": "^1.3.2"
	},
	"resolutions": {
		"@rushstack/eslint-patch": "^1.3.2"
	}
}
